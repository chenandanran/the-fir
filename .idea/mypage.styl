#top .navRight1{
  z-index :2;
  border-left :10px black solid;
  border-top:10px #000000 solid;
  border-bottom:20px #000000 solid;
  width:20%;
  height:200px;
  background:white;
  position:absolute;
  right:0;
  top:0;
  overflow :hidden;
}
#top .navRight1 ul{
  height:200px;
  list-style :none;
  position :absolute;
  top:0;
  left:0;
}
#top .navRight1 ul li {
  height: 200px;
  float: left;
  width:400px;
}
  #top .navRight2{
    z-index :2;
    overflow:hidden;
    border-right :15px black solid;
    border-left:15px #000000 solid;
    border-bottom:15px #000000 solid;
    border-top:15px #000000 solid;
    width:80%;
    height:200px;
    background: #000000;
    position:absolute;
    top:0;
    right :0;
  }
    #top .navRight2 ul{
      height:200px;
