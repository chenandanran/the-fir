#pic {
  z-index :2;
  position:absolute;
  top:0;
  left:35%;
  width:400px;
  height:400px;
  border:5px red solid;
  border-radius: 200px;
  text-align:center;
  line-height:400px;
  overflow:hidden;
}
#top #nav{
  z-index :1;
  background :url("../pic/1.jpg");
  border-radius :10px;
  border-bottom :30px #494949 solid;
  border-left:5px #000000 solid;
  border-right :20px #ffffff solid;
  position:relative;
    height:200px;
    width:60%;
  margin:0 auto;
  text-align center;
  }
#top #nav .round1,.round2,.round3,.round4,.round5,.round6{
  display:none;
  position: absolute;
  top:20px;
  left:0;
}
#top #nav .round7{
  margin-left:20px;
//  font-family:"свт╡";'
  display:block;
  text-align: center;
  z-index :2;
  width: 120px;
  height:120px;
  border:5px #ffffff solid;
  border-radius :60px;
  background: rgba(139, 0, 0, 0.68);
  position: absolute;
  top:20px;
  left:0px;
}
#nav .round7 a{
  line-height:120px;
  display:block;
  color: #ffffff;
  font-style italic;
  font-size:20px;
  letter-spacing:2px;
  text-decoration :none;
}
